{
   "manifest_version": 2,
   "name": "Zapier Classic",
   "icons": { 
      "16": "icon_16.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
   },
   "version": "0.1.0",
   "description": "Official Chrome Extension for interfacing with Zapier.com",
   "browser_action": {
      "default_icon": "icon_16.png",
      "default_popup": "popup.html"
   },
   "offline_enabled": false,
   "permissions": [
      "http://*.zapier.local/",
      "http://*.zapier.com/",
      "https://*.zapier.com/"
   ]
}